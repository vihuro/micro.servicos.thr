{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Routes": [
    //"AUTH.API
    {
      "DownstreamPathTemplate": "/api/v1/usuario",
